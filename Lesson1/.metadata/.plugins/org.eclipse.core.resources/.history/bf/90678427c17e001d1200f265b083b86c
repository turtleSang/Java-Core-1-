
public class XuLy {

	public XuLy() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public static void inLeChanDungFor () {
		final int A = 100;
		String soChan = "Số chẵn dưới 100 là: ";
		String soLe = "Số lẻ dưới 100 là: ";
		for (int i = 1; i < 100; i++) {
			if ((i%2) == 0) {
				soChan += i + "; ";
			} else {
				soLe += i  + "; ";
				
			}
		}
		
	}
}
