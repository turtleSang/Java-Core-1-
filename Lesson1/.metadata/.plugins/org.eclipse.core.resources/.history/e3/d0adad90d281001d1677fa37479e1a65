	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	//Hàm nhập n
	
	
	
	//Hàm tính giai thừa

	
	public static int timSoGiaiThua (int n) {
		int giaiThuaN = 0;
		for (int i = 1; i <= n && n != 0 && n != 1; i++) {
			giaiThuaN *= i;
		}
		
		return giaiThuaN;
	}
}
