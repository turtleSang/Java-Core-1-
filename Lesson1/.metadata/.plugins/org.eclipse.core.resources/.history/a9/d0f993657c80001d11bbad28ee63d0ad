
public class XuLy {

	public XuLy() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public static void printFibonacci (int num) {
		int numFi;
		for (int i = 0; i < num; i++) {
			if ( i == 0 || i == 1) {
				numFi = i;
			} else {
				numFi += numFi;
			}
		}
	}
}
